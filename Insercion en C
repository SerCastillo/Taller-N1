//Ordenamiento por Insercion

#include<stdio.h>
#include<conio.h>

int main(){
	
	int i,j,aux,t;

	printf("Tama√±o del array: ");
	scanf("%i",&t);
		int a[t];
	for(i=0;i<t;i++){
		printf("Digite numero en la posicion %i:  ",i);scanf("%i",&a[i]);
	}
	
	
	for(i = 0; i < t; i++){
            j = i;
            aux = a[i];
            while ((j > 0) && (aux < a[j - 1])) {
                a[j] = a[j - 1];
                j--;
            }
            a[j] = aux;
    }
	
	for(i=0;i<t;i++){
		printf("%i ",a[i]);
	}

	
	getch();
	return 0;
}
